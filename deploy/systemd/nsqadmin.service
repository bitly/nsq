[Unit]
Description= nsqadmin service file generated by Enso
Documentation=man:nsqadmin(1)
After=network.target

[Service]
User=cc
Group=cc
ExecStart=/opt/nsq-latest/bin/nsqadmin --lookupd-http-address=localhost:4161
Restart=on-failure
WorkingDirectory=/opt/nsq-latest/bin
StandardOutput=null
StandardError=null
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target
