[Unit]
Description= nsqd service file generated by Enso
Documentation=man:nsqd(1)
After=network.target

[Service]
User=cc
Group=cc
ExecStart=/opt/nsq-latest/bin/nsqd --max-msg-size=100000000 --mem-queue-size 20000 --lookupd-tcp-address=localhost:4160 --broadcast-address=127.0.0.1
Restart=on-failure
WorkingDirectory=/opt/nsq-latest/bin
StandardOutput=null
StandardError=null
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target
